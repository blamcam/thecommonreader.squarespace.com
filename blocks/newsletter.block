<div class="newsletter" role="contentinfo" sqs-controller="FooterBreakpoints">
  <div class="newsletter-inner clear">
    <squarespace:block-field id="footerBlocksTop" class="newsletter-blocks newsletter-blocks--top sqs-alternate-block-style-container" columns="12" label="Newsletter Top Blocks" />
    <div class="Footer-middle">
      <squarespace:block-field id="footerBlocksMiddle" class="newsletter-blocks newsletter-blocks--middle sqs-alternate-block-style-container" columns="12" label="Newsletter Middle Blocks" />
    </div>
    <squarespace:block-field id="footerBlocksBottom" class="newsletter-blocks newsletter-blocks--bottom sqs-alternate-block-style-container" columns="12" label="Newsletter Bottom Blocks" />
  </div>
</div>
